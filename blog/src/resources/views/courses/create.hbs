<div mt-4>
  <h2>Thêm mới khoá học</h2>
  <div class='row'>
    <form method='post'>
      <div class='form-group'>
        <input
          type='text'
          class='form-control'
          name='name'
          placeholder='Tên khoá học'
        />
      </div>
      <div class='form-group'>
        <input
          type='text'
          class='form-control'
          name='description'
          placeholder='Mô tả'
        />
      </div>
      <div class='form-group'>
        <input
          type='text'
          class='form-control'
          name='videoId'
          placeholder='VideoId'
        />
      </div>
      <button type='submit' class='btn btn-primary'>Submit</button>
    </form>
  </div>
</div>